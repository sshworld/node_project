<link rel="stylesheet" href="/stylesheets/input.css">


<form method="POST" action="/user/signUp" class="container">
    <div class="wrapper">
        <input name="user_id" type="text" required="required">
        <label for="">아이디</label>
    </div>

    <div class="wrapper">
        <input name="user_pw" type="text" required="required">
        <label for="">비밀번호</label>
    </div>

    <div class="wrapper">
        <input name="user_name" type="text" required="required">
        <label for="">이름</label>
    </div>

    <div class="wrapper">
        <input name="user_num" type="text" required="required">
        <label for="">전화번호</label>
    </div>

    <div class="wrapper">
        <input name="user_email" type="text" required="required">
        <label for="">이메일</label>
    </div>

    <div class="btn">
        <input type="submit" value="회원가입" style="font-size: 25px;">
    </div>

</form>